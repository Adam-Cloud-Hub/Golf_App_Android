<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Golf_App.Views.GameHistory"
             BackgroundColor="#132132"
             Shell.NavBarIsVisible="False">

    <ContentPage.Content>

        <Grid>

            <ListView x:Name="lv_View_Game_History" ItemSelected="lv_View_Game_History_ItemSelected" SelectionMode="Single">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding Path=CourseName}" TextColor="White"/>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>




        </Grid>

    </ContentPage.Content>
</ContentPage>